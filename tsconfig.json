{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "rootDir": "src/",
    "outDir": "/tmp/ts-moment",
    "target": "ES5",
    "lib": ["dom", "es6"],
    "module": "commonjs",
    "sourceMap": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "removeComments": false,
    "noImplicitAny": false,
    "pretty": true,
    "jsx": "react"
  },
  "exclude": [
    "node_modules"
  ]
}
